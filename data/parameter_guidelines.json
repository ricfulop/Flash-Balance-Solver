        {"name": "3YSZ", "formula": "Zr0.94Y0.06O1.97", "T_onset_typical": "900-1000°C"},
        {"name": "GDC10", "formula": "Ce0.9Gd0.1O1.95", "T_onset_typical": "650-750°C"}
      ],
      "thermodynamic_notes": "Use ZrO2 or CeO2 formation values, adjust for dopant"
