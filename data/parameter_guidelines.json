{
  "_description": "Flash Balance Parameter Guidelines by Material Family",
  "_version": "1.0",
  "_units": {
    "Ea": "eV",
    "sigma_0": "S/m",
    "beta": "dimensionless",
    "alpha_res": "dimensionless",
    "gamma": "dimensionless",
    "ksoft": "dimensionless",
    "delta_H": "J/mol",
    "delta_S": "J/(mol·K)",
    "r_eff": "m",
    "n_electrons": "electrons per reaction unit"
  },
  
  "families": {
    "fluorite": {
