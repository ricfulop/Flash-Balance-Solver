  "validation_criteria": {
    "excellent": {"error_percent": 5, "description": "< 5% error"},
    "good": {"error_percent": 10, "description": "5-10% error"},
    "acceptable": {"error_percent": 15, "description": "10-15% error"},
    "poor": {"error_percent": 999, "description": "> 15% error"}
  },
  
  "warning_flags": {
    "r_eff_too_small": {
      "threshold_um": 0.5,
      "message": "r_eff hit lower bound - other parameters may need adjustment"
    },
    "r_eff_too_large": {
      "threshold_um": 200,
      "message": "r_eff unusually large - verify thermodynamic data"
    },
    "high_field_extrapolation": {
      "message": "Model calibrated at one field may not extrapolate well to other fields"
    }
  }
