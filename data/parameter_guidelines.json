      "crystal_structure": "Cubic fluorite (Fm-3m)",
      "conduction_type": "Oxygen ion conduction",
      "parameters": {
        "Ea": {
          "typical": 0.95,
          "range": [0.7, 1.1],
          "notes": "Higher for tetragonal phase, lower for doped"
        },
        "sigma_0": {
          "typical": 3.6e4,
          "range": [1e4, 1e5],
          "notes": "Depends on dopant concentration"
        },
        "beta": {
          "typical": 1.69,
          "uncertainty": 0.10,
          "range": [1.5, 1.9],
          "source": "Flash Balance paper Table S1"
        },
        "alpha_res": {
          "typical": 0.15,
          "uncertainty": 0.03,
          "range": [0.12, 0.18],
          "source": "Flash Balance paper Table S1"
        },
        "gamma": {
          "typical": 2.0,
          "uncertainty": 0.2,
          "range": [1.8, 2.2]
        },
        "ksoft": {
          "typical": 0.10,
          "calculation": "1 - β × (0.73)²"
